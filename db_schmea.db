create table wallet_table(
        address_index integer,
        userId text,
        public_key text,
        token_symbol text,
        balance text,
        UNIQUE(userId, address_index, public_key),
        primary key(userId, address_index));


create table token_details(
        token_symbol text
        token_address text,
        chain_id text
)


create table transaction_table(
        id integer primary key autoincrement,
        type text,
        amount text,
        sender text,
        recepient text,
        txn_hash text,
        token_address text);

